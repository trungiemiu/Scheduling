{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run GA (module)",
      "type": "python",
      "request": "launch",
      "module": "fjssp_ga_cplex.ga.mainga",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "args": [
        "--data", "./fjssp_ga_cplex/data/Data.xlsx",
        "--pop", "10", "--gen", "50", "--mut", "0.1"
      ]
    },
    {
      "name": "Run CPLEX MIP",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/fjssp_ga_cplex/cplex_solver/cplex_solver.py",
      "cwd": "${workspaceFolder}/fjssp_ga_cplex",
      "console": "integratedTerminal",
      "args": ["--data", "./data/Data.xlsx"]
    }
  ]
}
